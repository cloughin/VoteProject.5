      <user:TextBoxWithNormalizedLineBreaks ID="TextBoxDomain2Add" runat="server" Width="365px" CssClass="TextBoxInput"></user:TextBoxWithNormalizedLineBreaks>&nbsp;<asp:Button
        ID="ButtonAddDomain" runat="server" CssClass="Buttons" Text="Add" Width="100px" OnClick="ButtonAddDomain_Click" /></td>
    </tr>
    </table>
    <!-- Table -->
    <table class="tableAdmin" id="Table2" cellspacing="0" cellpadding="0">
