html {
  background: #fff;
  min-width: 320px;
  overflow-y: auto;
}
body {
  font-family: 'Source Sans Pro', sans-serif;
  color: #000000;
  font-size: 16px;
  margin: 0;
  padding: 0;        
  min-width: 320px;
  min-height: 100%;
  text-align: center;
  background: #f8f8f8;
}
*,
*:before,
*:after {
  box-sizing: border-box;
}
.outer-page {
  width: 100%;
  max-width: 940px;
  margin: 0 auto;
  text-align: left;
  background: #ffffff;
}
img.responsive {
  width: 100%;
  height: auto;
}
.clearfix:after {
  content: "";
  display: table;
  clear: both;
}
.clearfixleft:after {
  content: "";
  display: table;
  clear: left;
}
.clearfixright:after {
  content: "";
  display: table;
  clear: right;
}
a,
a:link,
a:visited,
a:hover,
a:active {
  color: #0645ad;
  text-decoration: none;
}
a.republican {
  color: #d00000;
}
a.democrat {
  color: #0000d0;
}
a:hover {
  text-decoration: underline;
}
.slicknav_nav a:hover {
  text-decoration: none;
}
input[type=text],
input[type=email],
input[type=url],
input[type=number] {
  width: 100%;
  /*box-sizing: border-box;*/
  border: 1px solid #1a1a1a;
  border-radius: 0;
  font-size: 16px;
  font-weight: normal;
  line-height: 16px;
  padding: 10px 16px;
  color: #1a1a1a;
  font-family: 'Source Sans Pro', sans-serif;
  margin: 0;
  vertical-align: baseline;
}
input[type=submit],
a.link-button {
  border: 0;
  color: #ffffff;
  background: #ff6600;
  font-size: 18px;
  line-height: 22px;
  padding: 12px 20px;
  border-radius: 8px;
  font-weight: bold;
  margin: 0;
  font-family: sans-serif;
  text-decoration: none;
  display: inline-block;
  text-shadow: 1px 1px 1px #888;
}
input[type=submit]:hover,
a.link-button:hover {
  opacity: .80;
}
a.link-button {
  font-weight: normal;
  font-family: Georgia;
  padding: 8px 12px;
  border-radius: 5px;
}
a.link-button:after {
  content: " \25ba";
  font-family: sans-serif;
}
select {
  width: 100%;
  /*box-sizing: border-box;*/
  border: 1px solid #1a1a1a;
  border-radius: 0;
  font-size: 16px;
  font-weight: normal;
  line-height: 16px;
  padding: 10px 16px;
  color: #1a1a1a;
  font-family: 'Source Sans Pro', sans-serif;
  margin: 0;
  vertical-align: baseline;
}
textarea {
  width: 100%;
  /*box-sizing: border-box;*/
  border: 1px solid #1a1a1a;
  border-radius: 0;
  font-size: 16px;
  font-weight: normal;
  line-height: 16px;
  padding: 10px 16px;
  color: #1a1a1a;
  font-family: 'Source Sans Pro', sans-serif;
  margin: 0;
  vertical-align: baseline;
}

.js .no-js-only,
.no-js .js-only,
.only-print {
  display: none;
}

.ui-widget-overlay 
{
  background: repeat-x scroll 50% 50% #000;
}

.avoid-page-break {
  page-break-inside: avoid;
}

span.more-text {
  background-image: url(/images/more-text.gif);
  display: inline-block;
  width: 40px;
  height: 13px;
  position: relative;
  top: 3px;
  left: 2px;
  cursor: pointer;
}

span.more-ajax {
  background-image: url(/images/more-ajax.gif);
  display: inline-block;
  width: 40px;
  height: 13px;
  position: relative;
  top: 2px;
}

.ui-accordion-header.loading::after {
    content: url(/images/ajax-loader16.gif);
    position: relative;
    top: 3px;
    left: 8px;
}

/* donate-banner */
.donate-banner a {
  display: block;
  overflow: hidden;
}
    
/* social-media */
.social-media-outer {
  border-bottom: 1px solid #ccc; 
}
.social-media
{
  width: 100%;
  padding-top: 6px;
}
.social-media .addthis_toolbox,
.social-media .addthis_sharing_toolbox
{
  float: right;
  margin: 0 2px 6px 0;
}
.social-media .special-link {
  float: left;
  padding: 0 0 5px 5px;
}
.social-media .special-link .one-link {
  white-space: nowrap;
}
.social-media .special-link .one-link a {
  vertical-align: top;
  padding-top: 12px;
}
.social-media .special-link .link
{
  font-family: Arial;
  font-weight: bold;
  font-size: .7rem;
  padding-left: 4px;
  display: inline-block;
}
.social-media .special-link .sample-ballot-link .link
{
  color: green;
}
    
/* main-banner */
.main-banner-outer {
  margin: 10px 2px 0 2px;
}
    
.main-banner
{
  background: #fff;
  width: 100%;
}

.main-banner .logo
{
  float: left;
  margin: 0 0 4px 0;
  width: 45%;
  max-width: 280px;
}
    
.main-banner .logo img {
  width: 100%;
  height: auto;
}

.main-banner .state-banner
{
  font-family: Arial;
  margin-top: 3px;
  text-align: right;
  width: 100%;
  max-width: 100%;
}

.main-banner .state-banner .state-site
{
  text-align: right;
}
    
.main-banner .slicknav_nav 
{
  font-size: .86rem;
}
    
.main-banner .slicknav_menu {
  background: none;
}
    
.main-banner .slicknav_menu ul {
  z-index: 1;
  position: relative;
  top: -6px;
}
    
.main-banner .slicknav_menu li {
  background-color: #eee;
  display: inline-block;
  padding: 3px;
}
    
.main-banner .slicknav_menu li a {
  color:#234d9f;
  font-size: .85em;
  padding: 6px 0;
  margin: 0;
}
    
.js .main-banner .states-menu {
  height: 30px;
}
    
.js .main-banner .base-menu {
  display: none;
}
    
.no-js .main-banner .slicknav_menu {
  float: right;
  clear: right;
  top: 10px;
}
    
.no-js .main-banner .state-site {
  clear: right;
}
    
.no-js .main-banner .base-menu {
  display: inline-block;
  overflow: visible;
  right: 0;
}
    
.no-js .main-banner .base-menu ul {
  display: none;
  position: absolute;
  right: 0;
  padding-top: 60px;
}
    
.no-js .main-banner .base-menu:hover ul {
  display: block;
}
    
.no-js .main-banner .states-menu {
  position: relative;
}
    
.main-banner .states-menu-col {
  display: inline-block;
  margin-left: 15px;
}
    
.main-banner .states-menu-col:first-child {
  margin-left: 0;
}
   
.main-banner .slicknav_btn {
  margin: 0;
  padding: 0;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  background-color: transparent;
  font-size: 1.4rem;
}
   
.main-banner .slicknav_menu .slicknav_icon-bar 
{
  background-color: #234d9f;
}
   
.main-banner .slicknav_menutxt {
  text-shadow: none;
  color: #234d9f;
}
    
.main-banner .select-state-list {
  font-size: 1.4rem;
  font-weight: bold;
  font-family: Georgia, serif;
  color: #234d9f;
  border: none;
}

.main-banner .state-site 
{
  clear: right;
}

.main-banner .state-site a
{
  font-size: .83rem;
}

.main-banner .state-banner .not-associated
{
  color: #666;
  font-size: .6rem;
  padding-right: 15px;
}
    
/* main-navigation */
.main-navigation
{
  width: 100%;
  margin-top: 10px;
}
.js .main-navigation .no-js-menu {
   display: none;
}
.main-navigation ul.main-menu
{
  list-style: none;
  padding: 0;
  margin: 0;
}
.main-navigation ul.main-menu li
{
  float: left;
}
.main-navigation ul li a {
	text-indent: -10000px;
}
.main-navigation ul.main-menu li a
{
  display: block;
  height: 49px;
  overflow: hidden;
	font-size: 0;
	line-height: 0;
  background-size: contain;
  background-repeat: no-repeat;
  background-position: bottom;
}
.main-navigation .slicknav_menu {
  position: absolute;
  width: 100%;
}
.main-navigation ul.slicknav_nav li a
{
  height: 21px;
  margin: 1px 0 0 0;
}
.main-navigation ul li a:hover {
  opacity: .8;
}
.main-navigation ul.main-menu li.home 
{
  width: 13.5%;
}
.main-navigation ul.main-menu li.home a
{
  background-image: url(/images/nav-home.png);
}
.main-navigation ul.slicknav_nav li.home a
{
  background-image: url(/images/nav-home-h.png);
}
.main-navigation ul.main-menu li.for-voters 
{
  width: 18.6%;
}
.main-navigation ul.main-menu li.for-voters a
{
  background-image: url(/images/nav-voters.png);
}
.main-navigation ul.slicknav_nav li.for-voters a
{
  background-image: url(/images/nav-voters-h.png);
}
.main-navigation ul.main-menu li.for-candidates 
{
  width: 22.8%;
}
.main-navigation ul.main-menu li.for-candidates a
{
  background-image: url(/images/nav-candidates.png);
}
.main-navigation ul.slicknav_nav li.for-candidates a
{
  background-image: url(/images/nav-candidates-h.png);
}
.main-navigation ul.main-menu li.for-volunteers 
{
  width: 22.7%;
}
.main-navigation ul.main-menu li.for-volunteers a
{
  background-image: url(/images/nav-volunteers.png);
}
.main-navigation ul.slicknav_nav li.for-volunteers a
{
  background-image: url(/images/nav-volunteers-h.png);
}
.main-navigation ul.main-menu li.donate 
{
  width: 22.4%;
}
.main-navigation ul.main-menu li.donate a
{
  background-image: url(/images/nav-donate.png);
}
.main-navigation ul.slicknav_nav li.donate a
{
  background-image: url(/images/nav-donate-h.png);
  height: 28px;
}
.main-navigation .slicknav_menu {
  display: none;
  font-size: .81rem;
  /*box-sizing: border-box;*/
  background: #ffffff; 
  padding: 0;
  min-width: 320px;
}
.main-navigation .slicknav_menu .slicknav_menutxt {
  text-shadow: none;
  font-size: 75%;
}
.main-navigation .slicknav_menu li a {
  color: #000;
  padding: 0;
  margin: 0;
}
.main-navigation .slicknav_menu li a:hover {
  color: #000;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  background: none;
  }
   
.main-navigation .slicknav_btn {
  float: left;
  margin: 0;
  -webkit-border-radius: 0;
  -moz-border-radius: 0;
  border-radius: 0;
  background-color: #ffffff;
  text-align: right;
  width: 100%;
}
   
.main-navigation .slicknav_menutxt {
  float: none;
  display: inline-block;
  color: #234d9f;
}
   
.main-navigation .slicknav_icon {
  float: none;
  display: inline-block;
}
   
.main-navigation .slicknav_icon span {
  background-color: #234d9f;
}

/* page-heading */
.page-heading {
  width: 100%;
  padding-bottom: 10px;
  background-color: #365e77;
  color: #fff;
  font-family: Georgia;
  font-size: 24px;
  text-shadow: 1px 1px 1px #888;
}
.page-heading .title
{
  font-size: 24px;
  padding: 13px 0 0 12px;
}
.page-heading .subtitle
{
  font-size: 18px;
  padding: 8px 0 0 12px;
}

/* content */
.content {
  margin-top: -1px;
}
.content h2 {
  font-weight: bold;
  margin-top: 30px;
  font-size: 1.2rem;
}
.content h3 {
  font-weight: bold;
  margin-top: 25px;
  font-size: 1.1rem;
}
.content h4 {
  font-weight: bold;
  margin-top: 20px;
  font-size: 1rem;
}
.content .ui-widget-content {
  color: #000000;
}
.content .location-info {
  font-size: .95rem;
}
.content .location-info span {
  display: block;
  margin-left: 10px;
}
.content .location-info span.districts-head {
  margin-left: 0;
  font-weight: bold;
}
.content .election-title {
  font-size: 1.3rem;
  margin-top: 5px;
  font-weight: normal;
}
.content .office-title {
  margin-top: 5px;
  font-weight: normal;
}
.content .intro {
  margin: 2% 3%;
}
.content .intro p {
  margin: 10px 0 10px 0;
  line-height: 125%;
}   
.content .intro {
  padding-bottom: 0;
}
 
.responsive-report a.kalypto-checkbox
{
  background-image: url(/images/checkboxes.png);
  background-position: 0 -18px;
  display: block;
  height: 18px;
  width: 20px;
}

.responsive-report a.kalypto-checkbox.checked
{
  background-position: 0 0;
}
    
.ballot-checks-container a.candidate-checkbox {
  float: left;
  margin: 0 2px;
}
    
.ballot-checks-container a.kalypto-checkbox.checked {
  background-position: -60px 0;
}

/* intro-copy */
.intro-copy {
  margin: 4% 6%;
  font-family: Arial;
  font-size: 1rem;
  line-height: 130%;
}    
.intro-copy p {
  margin: 20px 0 10px 0;
}
.intro-copy .sub {
  margin: 0 40px;
}
.intro-copy ul {
  list-style-type: disc;
  margin-left: 24px;
}
.intro-copy li {
  margin-top: 3px;
}

/* vote-usa-address */

.vote-usa-address p {
  font-weight: bold;
  margin: .2em 0;
}

/* sample-ballot-button-box */
.sample-ballot-button-box {
  padding: 15px 20px;
  background: #F4F4F4;
  border: 1px solid #DFDFDF;
  border-radius: 4px;
  -moz-border-radius: 4px;
  width: 610px;
}
.sample-ballot-button-box .button-group {
  float: left;
}
.sample-ballot-button-box .style-x
{
  margin-top: 10px;
}
.sample-ballot-button-box .style-1,
.sample-ballot-button-box .style-4
{
  margin-top: 0;
}
.sample-ballot-button-box .button-group-2,
.sample-ballot-button-box .button-group-3
{
  margin-left: 24px;
}

/* address-entry */
#addressEntryInline {
  font-family: Arial;
  font-size: .75rem;
  line-height: .9rem;
}
#addressEntryInline input[type=text] {
  background: #eeeeee;
  margin-bottom: 2px;
}
#addressEntryInline input[type=submit] {
  display: block;
  margin: 30px 0 20px 0;
}
#addressEntryInline p.error-message {
  font-weight: bold;
  color: #d00000;
  padding: 0;
}
#addressEntryInline p.remember-me 
{
  margin-top: 20px;
  color: black;
}
    
