/* Alert Dialog                                */
/***********************************************/

.alert-dialog .ui-dialog-titlebar-close {
  display: none;
}

#alert-dialog .inner
{
  text-align: left;
  width: auto;
  min-width: 200px;
  max-width: 600px;
} 

#alert-dialog .message
{
