}

#tab-edittemplate .code-edit-heading .undo-redo div.undo
{
  margin-right: 2px;
  background-position: 0 0;
}

#tab-edittemplate .code-edit-heading .undo-redo div.undo:hover
{
  background-position: 0 -22px;
}

