/***********************************************/
/* Entire page                                 */
/***********************************************/

body.admin-page .mainlabel 
{
  color: #bf9241;
  font-weight: bold;
  text-align: left;
}

body.admin-page h2.jurisdiction-message
{
  margin: 0;
}

body.admin-page h4.credential-message
{
  font-size: 12px;
  font-style: italic;
}

body.admin-page .update-controls
{
  margin: 4px 0 0 0;
  position: relative;
}

#main-tabs
{
  margin: 0 0 20px 0;
  width: 933px;
}

#main-tabs .data-area
{
  min-height: 279px;
}

#main-tabs div.col
{
  float: left;
}

#main-tabs .ui-tabs-panel
{
  min-height: 509px;
}

#main-tabs div.htab-panel div.inner-panel
{
  min-height: 483px;
}
 
#main-tabs li.htab .tab-ast
{
  top: 0;
  left: 0;
}

#main-tabs .sublabel 
{
  color: #334f80;
  font-size: 8pt;
}

#main-tabs .mainlabel em
{
  color: #2254aa;
  font-style: normal;
}

#main-tabs .sublabel em
{
  color: #bf9241;
  font-style: normal;
  font-weight: bold;
}

#main-tabs div.control-box
{
  background: #eee;
  padding: 5px 0 5px 5px;
}

#main-tabs div.control-box label,
#main-tabs label.std-checkbox {
color: #334f80;
font-size: 8pt;
position: relative;
top: -2px;
}

#main-tabs .scrolling-list
{
  background-color: #ffe;
  border: 1px solid #dbdbdb;
  width: 260px;
  height: 100px;
  overflow-x: hidden;
  overflow-y: scroll;
  margin-bottom: 3px;
  color: #334f80;
}

#main-tabs .email-list input[type=text]
{
  width: 184px;
}

#main-tabs div.instructions 
{
  margin: 0 0 10px 15px;
}

#main-tabs div.instructions ul
{
  color: #bf9241;
  list-style: disc;
}

#main-tabs div.instructions ul ul
{
  margin-left: 15px;
  list-style: square;
}

#main-tabs div.instructions li
{
  margin-bottom: 2px;
}

#main-tabs div.instructions li span
{
  color: #334f80;
  font-size: 11px;
}

#main-tabs div.instructions em
{
  color: #bf9241;
  font-style: normal;
  font-weight: bold;
}
 
#main-tabs .save-options
{
  position: absolute;
  left: 15px;
  bottom: 10px;
}

#main-tabs p.template-name
{
  text-overflow: ellipsis;
	width: 858px;
	white-space: nowrap;
	overflow: hidden;
}

#main-tabs span.edittemplate-name
{
  color: #334f80;
}

body.admin-page .list-results
{
  border: 1px solid #888;
  overflow-y: scroll;
  height: 431px;
  margin-top: 8px;
}

body.admin-page .list-results p
{
  color: #334f80;
  font-size: 9pt;
  padding: 5px 5px 0 5px;
}

body.admin-page .list-results em
{
  color: #bf9241;
  font-style: normal;
  font-weight: bold;
}

body.admin-page .list-results table
{
  width: 100%
}

body.admin-page .list-results table.disabled
{
  opacity: 0.5;
  -moz-opacity: 0.5;
  filter: alpha(opacity: 50);
}

body.admin-page .list-results table div.css-checkbox
{
  float: left;
  margin: 1px 2px 0 0;
}

body.admin-page .list-results table thead tr
{
  background-color: #d8d8f9;
}

body.admin-page .list-results table tr.even
{
  background-color: #eef;
}

body.admin-page .list-results table tr.selected
{
  background-color: #00f;
}

body.admin-page .list-results table tr.selected td
{
  color: #fff;
}

body.admin-page .list-results table th,
body.admin-page .list-results table td
{
  padding: 0 3px
}

body.admin-page .list-results table th
{
  color: #bf9241;
  font-size: 8pt;
  border-left: 1px solid #eee;
}

body.admin-page .list-results table th:first-child
{
  border-left: none;
}

body.admin-page .list-results table td
{
  color: #334f80;
  font-size: 8pt;
}

body.admin-page .list-results .toggler div.criteria
{
  position: absolute;
  background: #ffe;
  width: 800px;
  padding: 2px 5px;
  display: none;
}

body.admin-page .list-results .toggler.showing div.criteria
{
  display: block;
}

body.admin-page .list-results .criteria p
{
  font-size: 8pt;
  padding-left: 20px;
  text-indent: -20px;
}

/* more-info is add-on to list-results for more info dialogs*/
body.admin-page div.more-info
{
  overflow-y: visible;
}

body.admin-page div.more-info table
{
  border: 1px solid #ccc;
}

body.admin-page div.more-info td.col1
{
  text-align: right;
  width: 125px;
  padding-right: 5px;
  font-weight: bold;
  color: #bf9241;
}

#main-tabs div input:disabled + label
{
  color: #999;
}

/***********************************************/
/* Edit Template                               */
/***********************************************/

#tab-edittemplate div.code-edit
{
  background-color: #ffe;
  border: 1px solid #dbdbdb;
}

#tab-edittemplate div.subject
{
  height: 32px;
}

#tab-edittemplate div.body
{
  height: 316px;
}

#tab-edittemplate .code-edit-heading .mainlabel
{
  float: left;
  padding-top: 7px;
}

