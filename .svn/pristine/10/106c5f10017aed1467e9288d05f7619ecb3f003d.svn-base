<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="Login.aspx.cs" 
Inherits="Vote.LoginPage" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html><head><title></title></head><body></body></html>


<%--@ Register TagPrefix="user" Namespace="Vote" Assembly="Vote" --%>
<%--@ Register Src="Banner.ascx" TagName="Banner" TagPrefix="user" --%>
<%--
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0 Transitional//EN" >
<HTML>
<head runat="server">
		<title>Login</title>
  <link href="/css/All.css" type="text/css" rel="stylesheet" />
  <link href="/css/AllAdmin.css" type="text/css" rel="stylesheet" />
		<meta content="Microsoft Visual Studio .NET 7.1" name="GENERATOR">
		<meta content="C#" name="CODE_LANGUAGE">
		<meta content="JavaScript" name="vs_defaultClientScript">
		<meta content="http://schemas.microsoft.com/intellisense/ie5" name="vs_targetSchema">
	</HEAD>
	<body>
		<form id="Form1" method="post" runat="server" defaultbutton="ButtonLogin" 
    defaultfocus="TextBoxUserName">
    <user:Banner ID="Banner" runat="server" />
    <!-- Table -->
			<TABLE class="tableAdmin" id="TableOuter" cellSpacing="0" cellPadding="0">
				<TR>
					<TD align="left" colSpan="2"><br />
						<asp:label id="Msg" runat="server" CssClass="TBoldColor"></asp:label><br />
            <br>
					</TD>
				</TR>
				<TR>
					<TD align="right" class="TBold">
            Username: &nbsp;
          </TD>
					<TD align="left"><user:TextBoxWithNormalizedLineBreaks id="TextBoxUserName" runat="server" EnableViewState="False" Width="350px" CssClass="TextBoxInputLogin" ></user:TextBoxWithNormalizedLineBreaks></TD>
				</TR>
				<tr>
					<td>
            &nbsp;</td>
				</tr>
				<TR>
					<TD align="right" class="TBold">
            Password: &nbsp;
          </TD>
					<TD align="left"><user:TextBoxWithNormalizedLineBreaks id="TextBoxPassword" runat="server" EnableViewState="False" TextMode="Password" Width="220px" CssClass="TextBoxInputLogin"></user:TextBoxWithNormalizedLineBreaks></TD>
				</TR>
				<tr>
					<td>
            &nbsp;</td>
				</tr>
				<TR>
					<td></td>
					<TD align="left">
            &nbsp;<asp:Button ID="ButtonLogin" runat="server" OnClick="ButtonLogin_Click"
              Text="Login" Width="114px" CssClass="Buttons" /></TD>
				</TR>
			</TABLE>
		</form>
	</body>
</HTML>
--%>