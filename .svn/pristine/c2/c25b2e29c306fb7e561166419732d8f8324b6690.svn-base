.menu-proxy
{
  display: none;
}

.no-js .menu-proxy
{
  display: block;
}

#outer .input
{
  border-color: #dbdbdb;
  display: block;
  height: 20px;
  width: 150px;
}

#outer .username
{
  margin-bottom: 10px;
}

#outer .field-label.has-ph
{
  display: none;
}

#outer .signin-container
{
  background: #eee;
  padding: 10px;
  width: 279px;
}

#outer .signin-container .col2
{
  margin: 0 0 0 10px;
}

#outer .signin-container .col2.no-ph
{
  margin: 18px 0 0 10px;
}

#outer .signin-container .feedback-container a.hider
{
  display: none;
}

#outer .signin-container .feedback-container .feedback
{
  background: #ffc;
  width: 272px;
}

#outer .col
{
  float: left;
}

#outer
{
  margin: 100px 0 0 30px;
  padding: 10px;
  width: 829px;
}

#outer .welcome
{
  margin: 0 30px 0 0;
  width: 498px;
}

#outer .welcome h2
{
  -moz-text-shadow: 1px 1px 0 #c8c8c8;
  -moz-text-shadow: 1px 1px 0 rgba(200, 200, 200, 0.7);
  -webkit-text-shadow: 1px 1px 0 #c8c8c8;
  -webkit-text-shadow: 1px 1px 0 rgba(200, 200, 200, 0.7);
  color: #2254aa;
  font-size: 28px;
  font-weight: bold;
  margin: 15px 0 15px 0;
  text-shadow: 1px 1px 0 #c8c8c8;
  text-shadow: 1px 1px 0 rgba(200, 200, 200, 0.7);
}

#outer .welcome p
{
  font-size: 18px;
  margin: 0 0 10px 0;
}

#outer .welcome p.p1
{
  color: #0b306f;
  line-height: 120%;
}

#outer .welcome p.p1.warn
{
  color: green;
}

#outer .welcome p.p1.error
{
  color: red;
}

#outer .welcome p.p2
{
  color: #888;
  font-size: 12px;
}