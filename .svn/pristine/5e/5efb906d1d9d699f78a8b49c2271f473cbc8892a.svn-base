namespace DB.Vote
{
  public partial class EmailBatches
  {
    public static EmailBatchesTable GetOpenBatchesData(int commandTimeout = -1)
    {
      var cmdText = SelectAllCommandText + " WHERE IsClosed=0 ORDER BY CreationTime";
      var cmd = VoteDb.GetCommand(cmdText, commandTimeout);
      return FillTable(cmd, EmailBatchesTable.ColumnSet.All);
    }
  }
}