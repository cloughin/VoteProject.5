/***********************************************/

/* upload tab                                  */

/***********************************************/

#main-tabs #tab-upload .tab-panel-heading
{
  margin-bottom: 5px;
}

#main-tabs #tab-upload .cols
{
  margin-top: 15px;
}

#main-tabs #tab-upload .col
{
  margin: 0 0 20px 0;
}

#main-tabs #tab-upload .col
{
  float: left;
}

#main-tabs #tab-upload .col1
{
  margin: 30px 80px 0 60px;
  padding: 20px;
  width: 337px;
}

#main-tabs #tab-upload .fileinputs
{
  margin: 18px 0 18px 25px;
}

#main-tabs #tab-upload .image-outer-container
{
  width: 302px;
}

#main-tabs #tab-upload .image-outer-container p.too-small
{
  background: #eee;
  display: none;
  font-size: 9px;
  line-height: 12px;
  margin: 10px;
  padding: 4px;
}

#main-tabs #tab-upload .image-container
{
  background: #ddd;
  border: 1px solid #888;
  min-height: 100px;
  width: 300px;
}

#main-tabs #tab-upload .image-picture
{
  display: block;
}

#main-tabs #tab-upload .fileinputs
{
  height: 24px;
  position: relative;
}

#main-tabs #tab-upload .masker
{
  background: #eee;
  display: none;
  position: absolute;
  z-index: 3;
}

#main-tabs #tab-upload .upload-file
{
  -moz-opacity: 0;
  cursor: pointer;
  filter: alpha(opacity: 0);
  opacity: 0;
  position: absolute;
  text-align: right;
  z-index: 2;
}

#main-tabs #tab-upload .browse-button
{
  font-size: 11px;
  left: 0;
  padding: 2px 18px;
  position: absolute;
  top: 0;
  z-index: 1;
}

#main-tabs #tab-upload .file-name-container
{
  left: 100px;
  position: absolute;
  top: 1px;
}

#main-tabs #tab-upload .file-name-clear
{
  background: #fff url(/images/opsprite.png) -100px 0;
  cursor: pointer;
  float: left;
  height: 20px;
  margin: 2px 0 0 5px;
  width: 20px;
}

#main-tabs #tab-upload .file-name-clear:hover
{
  background-position: -100px -20px;
}

#main-tabs #tab-upload .file-name
{
  background: #ffffee;
  border: 1px solid #2554aa;
  cursor: default;
  float: left;
  overflow: hidden;
  padding: 3px 5px;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 171px;
}

#main-tabs #tab-upload .file-name.disabled
{
  color: #bbb;
  font-style: italic;
}

#main-tabs #tab-upload .col1 .nx
{
  background: #fff url(/images/numbers.png);
  float: left;
  height: 20px;
  margin-right: 5px;
  width: 20px;
}

#main-tabs #tab-upload .col1 .n1
{
  background-position: 0 -20px;
}

#main-tabs #tab-upload .col1 .n2
{
  background-position: -20px -20px;
}

#main-tabs #tab-upload .col1 h6
{
  padding-top: 3px;
}

#main-tabs #tab-upload .col1 h6 em
{
  color: #0b306f;
  font-size: 110%;
  font-style: normal;
}

/***********************************************/

/* FIREFOX                                     */

/***********************************************/

@-moz-document url-prefix()
{
  #main-tabs #tab-social div.medium input
  {
    margin-top: 2px;
  }
  #main-tabs #tab-upload .browse-button
  {
    font-size: 11px;
    left: 0;
    padding: 1px 9px;
    top: 4px;
  }
  #main-tabs #tab-upload .upload-file
  {
    left: -142px;
    top: 2px;
  }
  #main-tabs #tab-upload .file-name-container
  {
    left: 82px;
  }
  #main-tabs #tab-upload .file-name
  {
    width: 189px;
  }
  div.vtab-panel .undo-button
  {
    right: 20px;
  }
  div.vtab-panel .clear-button
  {
    right: 50px;
  }
}

/***********************************************/

/* CHROME                                      */

/***********************************************/

@media
screen and (-webkit-min-device-pixel-ratio: 0)
{
  #main-tabs #tab-upload .browse-button
  {
    font-size: 11px;
    padding: 2px 18px;
    top: 1px;
  }

  #main-tabs #tab-upload .upload-file
  {
    left: 0;
    top: 0;
  }

  #main-tabs #tab-upload .masker2
  {
    display: block;
    height: 26px;
    left: 82px;
    opacity: 0;
    width: 190px;
  }
}

/***********************************************/

/* IE10                                        */

/***********************************************/

.ie10 #main-tabs #tab-social div.medium input
{
  margin-top: 2px;
}

.ie10 #main-tabs #tab-upload .masker2
{
  display: block;
  height: 26px;
  left: -155px;
  opacity: 0;
  width: 155px;
}

.ie10 #main-tabs #tab-upload .browse-button
{
  padding: 1px 21px;
  top: 5px;
}

.ie10 #main-tabs #tab-upload .upload-file
{
  left: -149px;
  top: 4px;
}

.ie10 #main-tabs #tab-upload .file-name-container
{
  left: 94px;
}

.ie10 #main-tabs #tab-upload .file-name
{
  width: 177px;
}