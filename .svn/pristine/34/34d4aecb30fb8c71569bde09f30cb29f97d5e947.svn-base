#outer h1
{
  margin-bottom: 10px;
}

div.content-header
{
  margin-top: 0;
}

#outer h1
{
  margin-bottom: 7px;
}

.main-heading
{
  float: left;
  width: 525px;
}

#outer .politician-links
{
  float: right;
  margin-top: 12px;
}

#outer .politician-links .actions
{
  float: left;
}

#outer .politician-links .intro-actions
{
  margin-right: 15px;
}

#outer .politician-links a.action
{
  border: 1px solid #bf9241;
  display: block;
  margin-bottom: 5px;
  padding: 1px 4px;
  text-decoration: none;
}

#outer .politician-links a.action:hover
{
  border-color: #ffaf1e;
  color: inherit;
  cursor: pointer;
  text-decoration: none;
}

#outer .politician-links a.action.disabled
{
  -moz-opacity: 0.3;
  cursor: default;
  filter: alpha(opacity: 30);
  opacity: 0.3;
}

#outer .politician-links a.action p
{
  color: #707070;
  font-family: "trebuchet ms", helvetica, sans-serif;
  font-size: 12px;
  line-height: 150%;
  margin-bottom: 0;
}

#outer .politician-links a.action p em
{
  color: #25a;
  font-size: 15px;
  font-style: normal;
  font-weight: bold;
}

#outer .politician-links a.action p em span
{
  color: #bf9241;
  font-size: 16px;
  font-weight: normal;
}

#outer .politician-links a.action ul
{
  color: #bf9241;
  list-style: disc;
  margin: 5px 0 0 50px;
}

#outer .politician-links a.action li
{
  color: #bf9241;
  font-family: "trebuchet ms", helvetica, sans-serif;
  font-size: 13px;
  line-height: 18px;
}

#outer .politician-links a.action li span
{
  color: #707070;
}

