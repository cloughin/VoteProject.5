/***********************************************/
/* Standard Styles                             */
/***********************************************/

.clear-both
{
  clear: both;
}

.hidden
{
  display: none;
}

.invisible
{
  visibility: hidden;
}

.unselectable {
  -moz-user-select: none;
  -webkit-user-select: none;
  -ms-user-select: none;
}

.selectable {
  -moz-user-select: text;
  -webkit-user-select: text;
  -ms-user-select: text;
}

.ellipsis
{
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}

.rounded
{
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
}

.rounded-border
{
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 1px solid #ccc;
  border-radius: 5px;
}

.shadow
{
  -moz-box-shadow: 3px 3px 4px #bbb;
  -webkit-box-shadow: 3px 3px 4px #bbb;
  box-shadow: 3px 3px 4px #bbb;
}

.shadow-2
{
  -moz-box-shadow: 2px 2px 3px #bbb;
  -webkit-box-shadow: 2px 2px 3px #bbb;
  box-shadow: 2px 2px 3px #bbb;
}

.shadow-3
{
  -moz-box-shadow: 1px 1px 3px #bbb;
  -webkit-box-shadow: 1px 1px 3px #bbb;
  box-shadow: 1px 1px 3px #bbb;
}

.gradient-bg-1,
.gradient-bg-1-hovering
{
  background: #fff4d3;
  background-image: -ms-linear-gradient(top, #FFFFEE 0, #FFE9B8 100%);
  background-image: -moz-linear-gradient(top, #FFFFEE 0, #FFE9B8 100%);
  background-image: -o-linear-gradient(top, #FFFFEE 0, #FFE9B8 100%);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #FFFFEE), color-stop(1, #FFE9B8));
  background-image: -webkit-linear-gradient(top, #FFFFEE 0, #FFE9B8 100%);
  background-image: linear-gradient(to bottom, #FFFFEE 0, #FFE9B8 100%);
}

.gradient-bg-1-hovering:hover
{
  background: #FFE9B8;
  background-image: -ms-linear-gradient(bottom, #FFFFEE 0, #FFE9B8 100%);
  background-image: -moz-linear-gradient(bottom, #FFFFEE 0, #FFE9B8 100%);
  background-image: -o-linear-gradient(bottom, #FFFFEE 0, #FFE9B8 100%);
  background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0, #FFFFEE), color-stop(1, #FFE9B8));
  background-image: -webkit-linear-gradient(bottom, #FFFFEE 0, #FFE9B8 100%);
  background-image: linear-gradient(to top, #FFFFEE 0, #FFE9B8 100%);
}

.gradient-bg-2
{
  background: #ddd;
  background-image: -ms-linear-gradient(top, #eee 0, #ccc 100%);
  background-image: -moz-linear-gradient(top, #eee 0, #ccc 100%);
  background-image: -o-linear-gradient(top, #eee 0, #ccc 100%);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #eee), color-stop(1, #ccc));
  background-image: -webkit-linear-gradient(top, #eee 0, #ccc 100%);
  background-image: linear-gradient(to bottom, #eee 0, #ccc 100%);
}

input[type=text].error,
input[type=password].error,
textarea.error,
