/***********************************************/
/* Vertical and horizontal centering           */
/***********************************************/

/* <div class="xxx-align"><div class="center-inner"><xx class="center-element" /></div></div>
/* need an explicit height for vert and width for horz  */

div.vert-center,
div.horz-center
{
  display: table;
  overflow: hidden;
  width: 100%; /* need an explicit height for vert-center */
}

div.vert-center div.center-inner
{
  display: table-cell;
  vertical-align: middle;
}

div.horz-center div.center-inner
{
  margin: 0 auto;
  text-align: center;
  width: 100%;
}	  

