// Initialize when ready
$(function ()
{
  $('.emailFormSubject').simpleCombo().focus();
  $('.emailFormSubmitButton').button();
  var errorLabel = $('.emailFormErrorLabel');
  var goodLabel = $('.emailFormGoodLabel');
  if (errorLabel.length != 0)
    alert(errorLabel.text());
  else if (goodLabel.length != 0)
    alert(goodLabel.text());
});
