<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="AddressEntryInline.ascx.cs" 
Inherits="Vote.Controls.AddressEntryInline" %>

<div class="address-entry-inline">
  <input id="Address" type="text" class="address-search" placeholder="Address or 9-digit zip" runat="server" />
  <p id="ErrorMessage" class="error-message" runat="server"></p>
  <input id="Remember" type="checkbox" class="remember-me" runat="server" /><span class="remember-label">Remember me on this device</span>
  <input id="Submit" type="submit" class="submit-button submit-address" value="Lookup Address &#x25ba;" />
</div>
