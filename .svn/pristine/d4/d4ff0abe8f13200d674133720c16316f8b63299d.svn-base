/***********************************************/
/* Standard Styles                             */
/***********************************************/

.clear-both
{
  clear: both;
}

.hidden
{
  display: none;
}

.invisible
{
  visibility: hidden;
}

#VoteReport .start-hidden
{
  display: none;
}

#VoteReport .rounded-border
{
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border: 1px solid #ccc;
  border-radius: 5px;
}

#VoteReport .shadow {
-moz-box-shadow: 3px 3px 4px #bbb;
-webkit-box-shadow: 3px 3px 4px #bbb;
box-shadow: 3px 3px 4px #bbb;
}

#VoteReport #MainTabs
{
  margin-bottom: 20px;
}

#VoteReport #MainTabs ul.htabs {
-khtml-border-bottom-left-radius: 0;
-khtml-border-bottom-right-radius: 0;
-moz-border-radius-bottomleft: 0;
-moz-border-radius-bottomright: 0;
-webkit-border-bottom-left-radius: 0;
-webkit-border-bottom-right-radius: 0;
background: #2254AA url(/images/tabbg.png) repeat-x;
background-image: -ms-linear-gradient(top, #779AD4 0, #2254AA 100%);
background-image: -moz-linear-gradient(top, #779AD4 0, #2254AA 100%);
background-image: -o-linear-gradient(top, #779AD4 0, #2254AA 100%);
background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #779AD4), color-stop(1, #2254AA));
background-image: -webkit-linear-gradient(top, #779AD4 0, #2254AA 100%);
background-image: linear-gradient(to bottom, #779AD4 0, #2254AA 100%);
border: 0;
border-bottom-left-radius: 0;
border-bottom-right-radius: 0;
}

#VoteReport #MainTabs ul.vtabs
{
  -khtml-border-bottom-left-radius: 4px;
  -khtml-border-bottom-right-radius: 0;
  -khtml-border-top-left-radius: 4px;
  -khtml-border-top-right-radius: 0;
  -moz-border-radius-bottomleft: 4px;
  -moz-border-radius-bottomright: 0;
  -moz-border-radius-topleft: 4px;
  -moz-border-radius-topright: 0;
  -webkit-border-bottom-left-radius: 4px;
  -webkit-border-bottom-right-radius: 0;
  -webkit-border-top-left-radius: 4px;
  -webkit-border-top-right-radius: 0;
  background: none;
  background-color: #f8f8f8;
  border: none;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 0;
  border-right: 1px solid #aaa;
  border-top-left-radius: 4px;
  border-top-right-radius: 0;
  bottom: 0;
  left: 0;
  padding: 5px 0 0 2px;
  padding-left: 0;
  position: absolute;
  top: 0;
  width: 150px;
}

#VoteReport #MainTabs li.tab
{
  -khtml-border-bottom-right-radius: 0;
  -khtml-border-top-left-radius: 4px;
  -moz-border-radius-bottomright: 0;
  -moz-border-radius-topleft: 4px;
  -webkit-border-bottom-right-radius: 0;
  -webkit-border-top-left-radius: 4px;
  background: #ddd;
  background-image: -ms-linear-gradient(top, #eee 0, #bbb 100%);
  background-image: -moz-linear-gradient(top, #eee 0, #bbb 100%);
  background-image: -o-linear-gradient(top, #eee 0, #bbb 100%);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #eee), color-stop(1, #bbb));
  background-image: -webkit-linear-gradient(top, #eee 0, #bbb 100%);
  background-image: linear-gradient(to bottom, #eee 0, #bbb 100%);
  border: 1px solid #aaa;
  border-bottom-right-radius: 0;
  border-top-left-radius: 4px;
  cursor: pointer;
  float: left;
  list-style: none;
  outline: 0;
  position: relative;
  top: 0;
  white-space: nowrap;
}

#VoteReport #MainTabs li.tab *
{
  cursor: pointer;
}

#VoteReport #MainTabs li.tab.ui-state-hover
{
  background: #ccc;
  background-image: -ms-linear-gradient(top, #bbb 0, #eee 100%);
  background-image: -moz-linear-gradient(top, #bbb 0, #eee 100%);
  background-image: -o-linear-gradient(top, #bbb 0, #eee 100%);
  background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #bbb), color-stop(1, #eee));
  background-image: -webkit-linear-gradient(top, #bbb 0, #bbb eee%);
  background-image: linear-gradient(to bottom, #bbb 0, #eee 100%);
}

#VoteReport #MainTabs li.tab.ui-state-active
{
  background: #fff;
  cursor: default;
}

#VoteReport #MainTabs li.tab.ui-state-active *
{
  cursor: default;
}

#VoteReport #MainTabs li.tab a
{
  background: transparent;
  color: #334f80;
  cursor: pointer;
  font-family: Arial;
  font-size: 9pt;
  font-weight: normal;
  line-height: 14px;
  text-decoration: none;
  text-shadow: 0 1px 1px #fff;
}

#VoteReport #MainTabs li.tab a *
{
  cursor: pointer;
}

#VoteReport #MainTabs li.tab.ui-state-hover a
{
  color: #0b306f;
}

#VoteReport #MainTabs li.tab.ui-state-active a
{
  color: #2254aa;
  font-weight: bold;
  text-shadow: none;
}

#VoteReport #MainTabs li.tab.ui-state-active a *
{
  cursor: default;
}

#VoteReport #MainTabs li.htab
{
  -khtml-border-bottom-left-radius: 0;
  -khtml-border-top-right-radius: 4px;
  -moz-border-radius-bottomleft: 0;
  -moz-border-radius-topright: 4px;
  -webkit-border-bottom-left-radius: 0;
  -webkit-border-top-right-radius: 4px;
  border-bottom: 1px solid #aaa;
  border-bottom-left-radius: 0;
  border-top-right-radius: 4px;
  margin: 1px 2px 0 0;
  padding: 0 2px;
}

#VoteReport #MainTabs li.htab.ui-state-active
{
  border-bottom: 1px solid #fff;
}

#VoteReport #MainTabs li.htab a
{
  float: left;
  font-size: 9pt;
  line-height: 14px;
  padding: 6px 12px;
  text-decoration: none;
}

#VoteReport #MainTabs li.htab a span
{
  display: block;
  font-size: 10px;
  line-height: 12px;
  margin-top: 3px;
}

#VoteReport #MainTabs li.vtab
{
  -khtml-border-bottom-left-radius: 4px;
  -khtml-border-top-right-radius: 0;
  -moz-border-radius-bottomleft: 4px;
  -moz-border-radius-topright: 0;
  -webkit-border-bottom-left-radius: 4px;
  -webkit-border-top-right-radius: 0;
  border-bottom-left-radius: 4px;
  border-top-right-radius: 0;
  left: 1px;
  margin: 0 0 5px 3px;
  overflow: hidden;
  padding: 0 1px 0 0;
  width: 144px;
}

#VoteReport #MainTabs li.vtab.ui-state-active
{
  border-right: 1px solid #fff;
  padding-right: 1px;
}

#VoteReport #MainTabs li.vtab a
{
  float: right;
  font-size: 8pt;
  line-height: 13px;
  padding: 2px 7px 2px 0;
  text-align: right;
  width: 135px;
  white-space: normal;
}

#VoteReport #MainTabs li.vtab .vert-center
{
  height: 30px;
}

#VoteReport #MainTabs div.tab-control
{
  -khtml-border-bottom-left-radius: 4px;
  -khtml-border-bottom-right-radius: 4px;
  -khtml-border-top-left-radius: 4px;
  -khtml-border-top-right-radius: 4px;
  -moz-border-radius-bottomleft: 4px;
  -moz-border-radius-bottomright: 4px;
  -moz-border-radius-topleft: 4px;
  -moz-border-radius-topright: 4px;
  -webkit-border-bottom-left-radius: 4px;
  -webkit-border-bottom-right-radius: 4px;
  -webkit-border-top-left-radius: 4px;
  -webkit-border-top-right-radius: 4px;
  background: #fff;
  border: 1px solid #aaa;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  font-family: Verdana, Arial, sans-serif;
  font-size: 9pt;
  line-height: 16px;
  position: relative;
}

#VoteReport #MainTabs div.htab-control
{
  margin-bottom: 12px;
  padding: 0;
}

#VoteReport #MainTabs div.vtab-control
{
  /*margin: 4px;*/
  padding: 0 0 0 160px;
  min-height: 500px;
}

#VoteReport div.tab-panel .tab-panel-heading
{
  width: 100%;
}

#VoteReport div.htab-panel
{
  padding: 5px 2px 2px 2px;
}

#VoteReport div.htab-panel div.inner-panel
{
  padding: 12px 18px;
  position: relative;
}

#VoteReport #MainTabs div.vtab-panel
{
  padding-top: 0;
}

  /***** For Headings *****/

#VoteReport div.vtab-panel div.vert-center,
#VoteReport div.vtab-panel div.horz-center
{
  height: 50px;
  width: 700px;
}

#VoteReport div.vtab-panel div.horz-center .center-inner,
#VoteReport div.vtab-panel div.vert-center .center-inner
{
  position: relative;
}

/*******/

#VoteReport #MainTabs  li.htab a
{
  font-size: 11pt;
}

#main-tabs div.vtab-panel
{
  min-height: 327px;
}
 div.vtab-panel
{
  min-height: 327px;
}
