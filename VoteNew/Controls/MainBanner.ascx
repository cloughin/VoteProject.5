<%@ Control Language="C#" AutoEventWireup="true" CodeBehind="MainBanner.ascx.cs" Inherits="VoteNew.Controls.MainBanner" %>

<div id="mainBanner">
  <div class="logo">
    <a id="MainBannerHomeLink" href="/" title="" runat="server"><img runat="server" id="MainBannerImageTag" src="/images/homelogo.png" alt="Vote USA - Connecting voters and candidates"/></a>
  </div>
  <div id="MainBannerStateBanner" class="stateBanner" runat="server">
    <div class="stateName"><img src="" id="MainBannerStateName" alt="" runat="server" /></div>
    <div class="stateSite">
      <a id="MainBannerStateSiteLink" class="stateSiteLink" href="#" title="" target="stateSite" runat="server">Official  Website →</a>
    </div>
    <div class="anotherState">
      <a class="anotherStateButton" title="Select another state" onclick="$('#addressEntryDialog').dialog('open');">Select another state</a>
    </div>
  </div>
</div>
